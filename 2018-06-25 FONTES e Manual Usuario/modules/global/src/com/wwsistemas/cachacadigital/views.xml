<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.wwsistemas.cachacadigital.entity.Produto"
          extends="_local"
          name="produto-com-categoria">
        <property name="idCategoria"
                  view="_minimal"/>
    </view>
    <view class="com.wwsistemas.cachacadigital.entity.Producao"
          extends="_local"
          name="producao-com-produto">
        <property name="produto"
                  view="_minimal">
            <property name="idCategoria"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.wwsistemas.cachacadigital.entity.Venda"
          extends="_local"
          name="venda">
        <property name="idProdutoVenda"
                  view="_minimal">
            <property name="idProduto"
                      view="_minimal">
                <property name="preco"/>
                <property name="idCategoria"
                          view="_minimal"/>
            </property>
            <property name="quantidade"/>
        </property>
        <property name="idCliente"
                  view="_minimal"/>
        <property name="total2"/>
    </view>
    <view class="com.wwsistemas.cachacadigital.entity.Produto_venda"
          extends="_local"
          name="produto_venda_view">
        <property name="idProduto"
                  view="_minimal">
            <property name="preco"/>
            <property name="idCategoria"
                      view="_minimal"/>
            <property name="quantidade"/>
        </property>
        <property name="venda"
                  view="_minimal">
            <property name="idProdutoVenda"
                      view="_minimal">
                <property name="idProduto"
                          view="_minimal">
                    <property name="preco"/>
                </property>
            </property>
        </property>
        <property name="custo"/>
    </view>
</views>
