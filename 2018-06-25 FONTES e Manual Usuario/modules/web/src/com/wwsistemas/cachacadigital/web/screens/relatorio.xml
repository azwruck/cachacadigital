<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://caption"
        class="com.wwsistemas.cachacadigital.web.screens.Relatorio"
        messagesPack="com.wwsistemas.cachacadigital.web.screens">
    <dsContext>
        <datasource id="clienteDs"
                    class="com.wwsistemas.cachacadigital.entity.Cliente"
                    view="_minimal"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout>
        <searchPickerField id="clienteField"
                           datasource="clienteDs"
                           property="cliente"/>
        <pickerField id="pickerField"
                     datasource="clienteDs"
                     property="cliente"
                     width="180px"/>
    </layout>
</window>
